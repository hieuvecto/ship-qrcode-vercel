# Development Mode - Copy this file to .env.local for local testing
# This configuration allows you to test the application without Google Cloud setup

# Enable mock data and skip authentication
USE_MOCK_DATA=true
SKIP_AUTH=true

# Public URL (required for QR codes)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Google Sheets Configuration (not needed when USE_MOCK_DATA=true)
# GOOGLE_SERVICE_ACCOUNT_EMAIL=
# GOOGLE_PRIVATE_KEY=
# GOOGLE_SHEET_ID=

# NextAuth Configuration (not needed when SKIP_AUTH=true)
# NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=123abc456def7890
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=

# Admin Configuration (not needed when SKIP_AUTH=true)
# ADMIN_EMAILS=
